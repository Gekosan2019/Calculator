<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="81dp"
        tools:layout_editor_absoluteY="26dp">

        <EditText
            android:id="@+id/string_11"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:ems="10"
            android:layout_gravity="center"
            android:inputType="textPersonName" />

        <EditText
            android:id="@+id/string_22"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:ems="10"
            android:layout_gravity="center"
            android:inputType="textPersonName" />

        <TextView
            android:id="@+id/string_33"
            android:layout_width="match_parent"
            android:layout_gravity="center"
            android:layout_height="50dp" />

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="581dp">

            <Button
                android:id="@+id/button22"
                android:layout_width="190dp"
                android:layout_height="285dp"
                android:layout_gravity="right"
                android:stateListAnimator="@null"
                android:visibility="visible"
                app:backgroundTint="@color/teal_700"
                tools:visibility="visible" />

            <Button
                android:id="@+id/button33"
                android:layout_width="190dp"
                android:layout_height="285dp"
                android:layout_gravity="left"
                android:stateListAnimator="@null"
                app:backgroundTint="@color/teal_700" />

            <Button
                android:id="@+id/button44"
                android:layout_width="190dp"
                android:layout_height="285dp"
                android:layout_gravity="bottom|right"
                android:stateListAnimator="@null"
                app:backgroundTint="@color/teal_700" />

            <Button
                android:id="@+id/button55"
                android:layout_width="190dp"
                android:layout_height="285dp"
                android:layout_gravity="bottom|left"
                android:stateListAnimator="@null"
                app:backgroundTint="@color/teal_700" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:shadowColor="#E91E63"
                android:text="+"
                android:textSize="150dp"
                android:translationX="105dp"
                android:translationY="40dp"
                android:visibility="visible"
                tools:visibility="visible" />

            <TextView
                android:id="@+id/textView6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:shadowColor="#E91E63"
                android:text="-"
                android:textSize="200dp"
                android:translationX="65dp"
                android:translationY="1dp"
                android:visibility="visible"
                tools:visibility="visible" />

            <TextView
                android:id="@+id/textView7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:shadowColor="#E91E63"
                android:text="*"
                android:textSize="150dp"
                android:translationX="105dp"
                android:translationY="360dp"
                android:visibility="visible"
                tools:visibility="visible" />

            <TextView
                android:id="@+id/textView8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:shadowColor="#E91E63"
                android:text="/"
                android:textSize="150dp"
                android:translationX="65dp"
                android:translationY="330dp"
                android:visibility="visible"
                tools:text="@string/textView8"
                tools:visibility="visible" />
        </FrameLayout>

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>